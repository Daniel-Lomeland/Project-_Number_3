CREATE TABLE COLOR
(
	COLOR_ID             SMALLINT NOT NULL AUTO_INCREMENT,
	COLOR_NAME           VARCHAR(25) NULL
) AUTO_INCREMENT = 1;

ALTER TABLE COLOR
