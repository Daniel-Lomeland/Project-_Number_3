CREATE TABLE OUTCOME_TYPE
(
	OUTCOME_TYPE_ID      SMALLINT NOT NULL AUTO_INCREMENT,
	OUTCOME_TYPE_NAME    VARCHAR(25) NULL
) AUTO_INCREMENT = 1;

ALTER TABLE OUTCOME_TYPE
