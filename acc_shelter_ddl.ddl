	ANIMAL_TYPE_ID       SMALLINT NOT NULL AUTO_INCREMENT,
	ANIMAL_TYPE_NAME     VARCHAR(25) NULL
) AUTO_INCREMENT = 1;

ALTER TABLE ANIMAL_TYPE
